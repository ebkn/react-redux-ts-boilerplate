{
  "name": "react-redux-ts-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for typesafe React + Redux + Typescript.",
  "main": "src/index.tsx",
  "author": "ebkn",
  "license": "MIT",
  "private": true,
  "scripts": {
    "typecheck": "tsc --noEmit",
    "prestart": "yarn typecheck",
    "start": "concurrently \"yarn webpack:dev\" \"yarn tsserver\"",
    "webpack:dev": "webpack-dev-server --watch --progress",
    "tsserver": "tsc --watch",
    "prebuild": "yarn typecheck",
    "build": "webpack",
    "prebuild:prod": "yarn typecheck",
    "build:prod": "webpack --mode production",
    "prelint": "yarn typecheck",
    "lint": "tslint './src/**/*.ts{,x}'",
    "prelint:fix": "yarn typecheck",
    "lint:fix": "tslint --fix './src/**/*.ts{,x}'",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook -c .storybook"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@storybook/react": "^4.1.11",
    "@types/node": "^11.13.4",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.8.0",
    "@types/react-redux": "^7.0.1",
    "@types/react-router-dom": "^4.3.1",
    "autoprefixer": "^9.1.5",
    "babel-loader": "^8.0.5",
    "concurrently": "^4.0.1",
    "css-loader": "^2.1.0",
    "normalize.css": "^8.0.1",
    "prettier": "^1.16.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0",
    "style-loader": "^0.23.1",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.1",
    "typescript-fsa": "^3.0.0-beta-2",
    "typescript-fsa-reducers": "^1.2.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "devDependencies": {
    "webpack-dev-server": "^3.1.14"
  }
}
